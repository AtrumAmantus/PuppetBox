    collar:
      rotation:
        x: -11.25
    neck:
      rotation:
        x: 22.5
    head:
      rotation:
        x: -11.25
  22:
    collar:
      rotation:
        x: 11.25
    neck:
      rotation:
        x: -22.5
    head:
      rotation:
        x: 11.25